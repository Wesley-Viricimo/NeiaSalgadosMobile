{
  "expo": {
    "name": "NeiaSalgadosMobile",
    "slug": "NeiaSalgadosMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neiasalgados.mobile",
      "buildNumber": "1",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.XXXX"
        }
      },
      "pushNotification": {
        "enabled": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.neiasalgados.mobile",
      "config": {
        "googleServicesFile": "./google-services.json"
      },
      "pushNotification": {
        "enabled": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "API_URL": "http://10.0.2.2:3000/api/v1"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#000000"
        }
      ]
    ]
  }
}